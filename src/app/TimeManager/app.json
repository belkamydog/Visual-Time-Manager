{
  "configVersion": "v3",
  "app": {
    "appId": 1099579,
    "appName": "VisualTimePlanner",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "Visual time planner application"
  },
  "permissions": ["device:os.bg_service"],
  "runtime": {
    "apiVersion": {
      "compatible": "4.0",
      "target": "4.0",
      "minVersion": "4.0"
    }
  },
  "targets": {
    "default": {
      "module": {
        "page": {
          "pages": [
            "page/index",
            "page/event",
            "page/list",
            "page/menu",
            "page/about",
            "page/settings/menu",
            "page/settings/history",
            "page/event/create/description",
            "page/event/create/start_date",
            "page/event/create/end_date",
            "page/event/create/color",
            "page/event/create/repeat",
            "page/event/edit/menu",
            "page/event/edit/description",
            "page/event/edit/start_date",
            "page/event/edit/end_date",
            "page/event/edit/color",
            "page/event/edit/repeat"
          ]
        },
        "app-service": {
          "services": ["app-service/service"]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "VisualTimePlaner"
    }
  },
  "defaultLanguage": "en-US"
}
